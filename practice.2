#include<stdio.h>
#include<math.h>

int main()

{
    int   i, j, k, n = 0;
    for (i = 100; i <= 200; i++)
    {
        k = 1;
        for (j = 2; j < i; j++)
            if (i%j==0) { k = 0; break; }
        if (k == 1)
        {
            n++;
            printf("%d\n", i);
        }
    }
    printf("%d\n", n);
    return 0;
}

#include<stdio.h>
#include<math.h>

int main()
{
    int  m, n, r ;
    printf("请输入两个正整数:");
    scanf("%d %d", &m, &n);
    do
    {
      r = m % n;
      m = n;
      n = r;
    } while (r != 0);

    printf("gcd=%d", m);
    return 0;
}

