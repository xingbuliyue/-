#include<stdio.h>
#include<math.h>

int main()
{
    int  i, m, s, n=0;

    for (i = 1; i <= 100; i++)

    {

        m = i;

        s = m%10;

        while (m > 0)

        {
            s = s +m/10 ;

            m = m / 10;

        }

        if (s == 5) 
        { 
            n++;
            printf("%d\n", i);
        }

    }

    printf("%d", n);
    return 0;
}

